{
    // Basics
    "window.zoomLevel": 0,
    "window.menuBarVisibility": "toggle",
    "workbench.activityBar.visible": false,
    "workbench.startupEditor": "none",
    "workbench.sideBar.location": "left",
    "workbench.settings.openDefaultSettings": false,
    "workbench.settings.openDefaultKeybindings": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmDelete": false,
    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,

    // Editor
    "editor.fontFamily": "'SF Mono','monospace', 'Droid Sans Mono', monospace, 'Droid Sans Fallback'",
    "editor.smoothScrolling": false,
    "editor.wordWrap": "on",
    "editor.wordWrapColumn": 80,
    "editor.wrappingIndent": "same",
    "editor.tabSize": 4,
    "editor.rulers": [
        80
    ],
    "editor.multiCursorModifier": "ctrlCmd",

    // Terminal
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.rendererType": "dom",
    "terminal.integrated.showExitAlert": false,

    // Markdown
    "markdown.preview.scrollPreviewWithEditor": true,
    "markdown.preview.scrollEditorWithPreview": true,

    // Debug
    "debug.internalConsoleOptions": "openOnSessionStart",
    "debug.toolBarLocation": "docked",
    
    // Python
    "python.formatting.provider": "black",
    "python.pythonPath": "python3",

    // C/C++ 
    "C_Cpp.formatting": "Disabled",

    // Vim
    "vim.overrideCopy": true, // def false
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "vim.leader": "<space>",
    "vim.handleKeys": {
        "<C-a>": true,
        "<C-b>": false,
        "<C-f>": false,
        // "<C-c>": false,
        "<C-x>": false,
    },
    "vim.normalModeKeyBindingsNonRecursive": [
        {
        "before": ["<leader>", "a"],
        "after": ["m", "l", "g", "g", "V", "G"]
        },
        {
        "before": ["<leader>", "<leader>"],
        "commands": [":w"]
        },
        {
        "before": ["<C-n>"],
        "commands": [":nohl"]
        },
    ],

    // Terminal 
    "workbench.colorCustomizations": {
        "editorRuler.foreground": "#363636",
        "terminal.background": "#181818",
        "terminal.foreground": "#D8D8D8",
        "terminalCursor.background": "#D8D8D8",
        "terminalCursor.foreground": "#D8D8D8",
        "terminal.ansiBlack": "#181818",
        "terminal.ansiBlue": "#7CAFC2",
        "terminal.ansiBrightBlack": "#585858",
        "terminal.ansiBrightBlue": "#7CAFC2",
        "terminal.ansiBrightCyan": "#86C1B9",
        "terminal.ansiBrightGreen": "#A1B56C",
        "terminal.ansiBrightMagenta": "#BA8BAF",
        "terminal.ansiBrightRed": "#AB4642",
        "terminal.ansiBrightWhite": "#F8F8F8",
        "terminal.ansiBrightYellow": "#F7CA88",
        "terminal.ansiCyan": "#86C1B9",
        "terminal.ansiGreen": "#A1B56C",
        "terminal.ansiMagenta": "#BA8BAF",
        "terminal.ansiRed": "#AB4642",
        "terminal.ansiWhite": "#D8D8D8",
        "terminal.ansiYellow": "#F7CA88"
    },

    // https://github.com/Microsoft/vscode/issues/32579
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                // inspect TM scopes 
                "scope": [
                    "comment",
                    "punctuation.definition.comment",
                    "variable.language",
                    "entity.other.attribute-name",
                ],
                "settings": {
                    "fontStyle": ""
                }
            }
        ]
    },
    
    // Workaround (Terminal)
    "terminal.integrated.commandsToSkipShell": [
        "editor.action.toggleTabFocusMode",
        "workbench.action.debug.continue",
        "workbench.action.debug.pause",
        "workbench.action.debug.restart",
        "workbench.action.debug.run",
        "workbench.action.debug.start",
        "workbench.action.debug.stepInto",
        "workbench.action.debug.stepOut",
        "workbench.action.debug.stepOver",
        "workbench.action.debug.stop",
        "workbench.action.firstEditorInGroup",
        "workbench.action.focusActiveEditorGroup",
        "workbench.action.focusEighthEditorGroup",
        "workbench.action.focusFifthEditorGroup",
        "workbench.action.focusFirstEditorGroup",
        "workbench.action.focusFourthEditorGroup",
        "workbench.action.focusLastEditorGroup",
        "workbench.action.focusSecondEditorGroup",
        "workbench.action.focusSeventhEditorGroup",
        "workbench.action.focusSixthEditorGroup",
